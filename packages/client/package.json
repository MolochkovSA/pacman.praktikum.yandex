{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "dev:csr": "cross-env VITE_MODE=development vite",
    "dev": "npm run build:sw && npm run build:server && cross-env VITE_MODE=development node dist/index.js",
    "build": "npm run build:sw && npm run build:server && npm run build:client && npm run build:ssr",
    "build:sw": "tsc -p tsconfig.worker.json",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "node dist/index.js",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" && stylelint \"**/*.scss\"",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:scss": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@reduxjs/toolkit": "^2.8.1",
    "bootstrap": "^5.3.6",
    "client": "file:",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsdom": "^26.1.0",
    "phosphor-react": "^1.4.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.2",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.30.1",
    "redux": "^5.0.1",
    "sass": "^1.87.0",
    "serialize-javascript": "^6.0.2",
    "vite": "^6.3.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.31",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "lefthook": "^1.11.12",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "stylelint": "16.19.1",
    "stylelint-config-standard-scss": "14.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
